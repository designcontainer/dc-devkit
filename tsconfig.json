{
	"compilerOptions": {
		"outDir": "./bin/",
		"target": "es5",
		"module": "es6",
		"rootDir": "./src",
		"moduleResolution": "Node",
		"esModuleInterop": true,
        "lib": ["es2015", "dom"],
        "allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
